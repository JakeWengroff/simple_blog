<!DOCTYPE html>
<html>
<head>
  <title>Dummy</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  <% if content_for?(:seo_meta) %>
    <%= yield(:seo_meta) %>
  <% end %>
</head>
<body>

<% if notice %>
  <%= notice %>
<% elsif alert %>
  <%= alert %>
<% end %>

<%= yield %>

</body>
</html>
